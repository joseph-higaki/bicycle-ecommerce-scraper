<div style="text-align:center">
  <span class="dotbici" onclick="currentSlide1(1)"></span> 
  <span class="dotbici" onclick="currentSlide1(2)"></span> 
</div>

<script>
var slideIndexUno = 1;
showSlides1(slideIndexUno);

function plusSlides(n) {
  showSlides1(slideIndexUno += n);
}

function currentSlide1(n) {
  showSlides1(slideIndexUno = n);
}

function showSlides1(n) {
  var i;
  var slidesbici = document.getElementsByClassName("mySlidesbici");
  var dotsbici = document.getElementsByClassName("dotbici");
  if (n > slidesbici.length) {slideIndexUno = 1}    
  if (n < 1) {slideIndexUno = slidesbici.length}
  for (i = 0; i < slidesbici.length; i++) {
      slidesbici[i].style.display = "none";  
  }
  for (i = 0; i < dotsbici.length; i++) {
      dotsbici[i].className = dotsbici[i].className.replace(" active", "");
  }
  slidesbici[slideIndexUno-1].style.display = "block";  
  dotsbici[slideIndexUno-1].className += " active";
}
</script>
</div>
