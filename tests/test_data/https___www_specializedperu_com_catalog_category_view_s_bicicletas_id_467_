/* The dots/bullets/indicators */
.dotbici {
  cursor: pointer;
  height: 10px;
  width: 10px;
  margin: 0 2px;
  background-color: #bbb;
  border-radius: 50%;
  display: inline-block;
  transition: background-color 0.6s ease;
}

.slideshow-container .active, .dotbici:hover {
  background-color: #717171;
}

</style>

<div class="slideshow-container">

<div class="mySlidesbici fade">
  <div class="numbertext">1 / 2</div>
  <img src="https://www.specializedperu.com/pub/media/wysiwyg/bicis-1.jpg" style="width:100%">
  <div class="text">ROAD BIKES</div>
</div>

<div class="mySlidesbici fade">
  <div class="numbertext">2 / 2</div>
