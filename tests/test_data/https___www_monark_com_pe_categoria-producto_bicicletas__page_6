  $("#fprecio").click(function(){
    $(".fpprecio").toggle();
  });
  $("#fmarca").click(function(){
    $(".fpmarca").toggle();
  });
  $("#aro").click(function(){
    $(".aro").toggle();
  });
  $("#genero").click(function(){
    $(".genero").toggle();
  });
});
</script>
