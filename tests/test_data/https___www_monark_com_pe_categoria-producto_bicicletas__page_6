<script>
function collapseNavbar() {
	if ($(".buscar-carrito").offset().top > 50) {
		$(".buscar-carrito").addClass("shrink");
    $(".menuppal.is_active").addClass("shrinke");
	} else {
		$(".buscar-carrito").removeClass("shrink");
    $(".menuppal.is_active").removeClass("shrinke");
	}
}
$(window).scroll(collapseNavbar);
$(document).ready(collapseNavbar);
</script>
<script>
const second = 1000,
      minute = second * 60,
      hour = minute * 60,
      day = hour * 24;
let countDown = new Date('July 23, 2020 00:00:00').getTime(),
    x = setInterval(function() {
      let now = new Date().getTime(),
          distance = countDown - now;
        document.getElementById('h').innerText = Math.floor(distance / (day)*24),
        document.getElementById('m').innerText = Math.floor((distance % (hour)) / (minute)),
        document.getElementById('s').innerText = Math.floor((distance % (minute)) / second);
    }, second)
</script>
