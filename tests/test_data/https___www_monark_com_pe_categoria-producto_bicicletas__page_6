<script>
    $(document).ready(function () {
        var form = $('#frm-filter-brand');
        var contentBrands = $('.filter_brands');
        contentBrands.find('input').change(function () {
            var valueBrands = '';
            contentBrands.find('input').each(function (n) {
                if ($(this).is(':checked')) {
                    valueBrands += n > 0 ? '+' : '';
                    valueBrands += $(this).val();
                }
            });

            form.find('input[name=brand]').val(valueBrands);
            form.submit();
        });

        var width = $(window).width();
        var height = 0;
        if (width > 768) {
            setTimeout(function () {
                $('.owl-item').each(function () {
                    if (height < $(this).height()) {
                        height = $(this).height();
                    }
                });
                $('.owl-item').height(height);
            }, 1000);
        }
        $(document).ready(function () {
                            $("input[name=aro]").change(function(){
                                var item=$(this);
                                if(item.is(":checked"))
                                {
                                    window.location.href= item.data("target");
                                }
                                });
                        });
    });
</script>
<script>
$(document).ready(function(){
